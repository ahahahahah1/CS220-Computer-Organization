    .scope S_0x555d053b0780;
T_4 ;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x555d053dcde0_0, 0;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x555d053dcc50_0, 0;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x555d053dcb90_0, 0;
T_4.0 ;
    %delay 200, 0;
    %load/vec4 v0x555d053dcc50_0;
    %inv;
    %store/vec4 v0x555d053dcc50_0, 0, 1;
    %jmp T_4.0;
    %end;
    .thread T_4;
    .scope S_0x555d053b0780;
T_5 ;
    %delay 5, 0;
    %load/vec4 v0x555d053dcb90_0;
    %inv;
    %store/vec4 v0x555d053dcb90_0, 0, 1;
    %jmp T_5;
    .thread T_5;
    .scope S_0x555d053b0780;
T_6 ;
    %delay 1000, 0;
    %vpi_call 2 17 "$finish" {0 0 0};
    %end;
    .thread T_6;
    .scope S_0x555d053b0780;
T_7 ;
    %wait E_0x555d053c11c0;
    %vpi_call 2 23 "$display", "at time = %d, %d %d %d %d", $time, &PV<v0x555d053dcd10_0, 3, 1>, &PV<v0x555d053dcd10_0, 2, 1>, &PV<v0x555d053dcd10_0, 1, 1>, &PV<v0x555d053dcd10_0, 0, 1> {0 0 0};

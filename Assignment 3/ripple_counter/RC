:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x555d053b0780 .scope module, "ripple_counter_tb" "ripple_counter_tb" 2 3;
 .timescale 0 0;
v0x555d053dcb90_0 .var "clk", 0 0;
v0x555d053dcc50_0 .var "flag", 0 0;
v0x555d053dcd10_0 .net "q", 3 0, L_0x555d053dd2c0;  1 drivers
v0x555d053dcde0_0 .var "rst", 0 0;
E_0x555d053c11c0 .event edge, v0x555d053dc870_0;
S_0x555d053c3010 .scope module, "RC1" "ripple_counter" 2 20, 3 4 0, S_0x555d053b0780;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk"
    .port_info 1 /INPUT 1 "flag"
    .port_info 2 /INPUT 1 "reset"
    .port_info 3 /OUTPUT 4 "q"
v0x555d053dc710_0 .net "clk", 0 0, v0x555d053dcb90_0;  1 drivers
v0x555d053dc7d0_0 .net "flag", 0 0, v0x555d053dcc50_0;  1 drivers
v0x555d053dc870_0 .net "q", 3 0, L_0x555d053dd2c0;  alias, 1 drivers

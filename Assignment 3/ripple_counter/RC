o0x7f2ff8105078 .functor BUFZ 1, C4<z>; HiZ drive
v0x555d053dca20_0 .net "rst", 0 0, o0x7f2ff8105078;  0 drivers
L_0x555d053dcf10 .part L_0x555d053dd2c0, 0, 1;
L_0x555d053dd060 .part L_0x555d053dd2c0, 1, 1;
L_0x555d053dd160 .part L_0x555d053dd2c0, 2, 1;
L_0x555d053dd2c0 .concat8 [ 1 1 1 1], v0x555d053b0ba0_0, v0x555d053db7a0_0, v0x555d053dbdd0_0, v0x555d053dc430_0;
S_0x555d053c31e0 .scope module, "tf1" "t_flipflop" 3 33, 4 3 0, S_0x555d053c3010;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "t"
    .port_info 1 /INPUT 1 "clk"
    .port_info 2 /OUTPUT 1 "q"
    .port_info 3 /INPUT 1 "rst"

v0x555d053dc350_0 .net "clk", 0 0, L_0x555d053dd160;  1 drivers
v0x555d053dc430_0 .var "q", 0 0;
v0x555d053dc4f0_0 .net "rst", 0 0, o0x7f2ff8105078;  alias, 0 drivers
L_0x7f2ff80bc0f0 .functor BUFT 1, C4<1>, C4<0>, C4<0>, C4<0>;
v0x555d053dc5c0_0 .net "t", 0 0, L_0x7f2ff80bc0f0;  1 drivers
E_0x555d053c1800 .event negedge, v0x555d053dc350_0;
    .scope S_0x555d053c31e0;
T_0 ;
    %wait E_0x555d053c12d0;
    %load/vec4 v0x555d053af8a0_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x555d053b0ba0_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x555d053db300_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.2, 8;
    %load/vec4 v0x555d053b0ba0_0;
    %inv;
    %assign/vec4 v0x555d053b0ba0_0, 0;
T_0.2 ;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x555d053db440;
T_1 ;
    %wait E_0x555d053c16a0;
    %load/vec4 v0x555d053db860_0;
